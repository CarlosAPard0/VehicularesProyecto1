    <!-- Calle vertical (n3 -> center -> n4) -->
    <edge id="n3_to_center" from="n3" to="center" numLanes="2" speed="13.89" />
    <edge id="center_to_n4" from="center" to="n4" numLanes="2" speed="13.89" />

    <!-- Sentido opuesto (n4 -> center -> n3) -->
    <edge id="n4_to_center" from="n4" to="center" numLanes="2" speed="13.89" />
    <edge id="center_to_n3" from="center" to="n3" numLanes="2" speed="13.89" />
</edges>
