    <edge id="n1_to_center" from="n1" to="center" numLanes="2" speed="13.89" />
    <edge id="center_to_n2" from="center" to="n2" numLanes="2" speed="13.89" />

    <!-- Sentido opuesto (n2 -> center -> n1) -->
